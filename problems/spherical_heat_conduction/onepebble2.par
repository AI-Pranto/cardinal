#
# nek parameter file
#
[GENERAL] 
polynomialOrder = 7
startFrom = onepebble2.run05
numSteps = 10000
dt = 1e-03
writeInterval = 100
extrapolation = OIFS
targetCFL = 2.0

[PRESSURE]
preconditioner = semg_amg
amgSolver = parAlmond
residualTol = 1e-04
residualProj = no

[VELOCITY]
boundaryTypeMap = W, O, v
residualTol = 1e-04
density = 2.0
viscosity = 0.5

[TEMPERATURE] # temperature with Hmholtz 
boundaryTypeMap = f, I, t
rhoCp = 100.0
conductivity = 2.0
residualTol = 1e-03
residualProj = yes

