      parameter (lsurf_m=1000)
      common /point_cloud/ pc_x(lsurf_m*4), pc_y(lsurf_m*4), 
     & pc_z(lsurf_m*4), pc_flag(lsurf_m*4)
      common /data_reference/ plmask(lelt), pflmask(lelt), 
     & pplist(lelt), offset, nw_bd1, nw_bdt
      common /data_cloud/ pc_t(lsurf_m), pc_f(lsurf_m)
      common /test_passing/ flux_moose, temp_nek
      real pc_x, pc_y, pc_z, pc_flag, pc_t, pc_f
      integer plmask, pflmask, pllist, offset, nw_bd1, nw_bdt 

